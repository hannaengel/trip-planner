<h1>Ready for your next trip? Lets Plan it now!</h1>


<%= form_for @trip do |f| %>
<%= render 'errors' %>
<%= f.label :name %> <%= f.text_field :name %><br>
<%= f.label :destination %> <%= f.text_field :destination %><br>
<%= f.label :user_id %> <%= f.collection_select(:user_id, User.all, :id, :username) %><br>

<%= f.submit %>
<% end %>
